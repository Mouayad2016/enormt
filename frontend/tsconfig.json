{
  "compilerOptions": {
    
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "awesomeTypescriptLoaderOptions": {
      "useWebpackText": true,
      "useTranspileModule": true,
      "doTypeCheck": true,
      "forkChecker": true
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/components/meta/PageMeta.tsx", "src/components/section/section.tsx", "src/pages/service/[title].jsx", "src/components/header/PageHeader.jsx", "src/components/header/Navbar.jsx", "src/pages/contact.jsx", "src/components/hem/service.tsx", "src/pages/404.jsx", "src/pages/index.jsx", "src/pages/_document.jsx", "src/pages/_app.jsx"],
  "exclude": ["node_modules"]

  
}
